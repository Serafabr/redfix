# Node.js environment
NODE_ENV=development

# HTTP port
HTTP_PORT=3001

# Requests logging
# LOG_OUTPUT:    none, console, file or both
LOG_OUTPUT=console

# Database connection
PGADMINUSER=administrator
PGADMINPASS=123456
PGUSER=administrator
PGPASSWORD=123456
PGHOST=localhost
PGPORT=5432
PGDATABASE=db_dev

# Cookie
COOKIE_NAME=cmms:session
COOKIE_KEY_0=secret0
COOKIE_KEY_1=secret1
COOKIE_KEY_2=secret2
COOKIE_MAX_AGE=604800000

# Sessions
# SESSION_SOURCE:            default or cookie
# SESSION_DEFAULT_PERSON_ID: a person_id from the database
# SESSION_DEFAULT_ROLE:      administrator, supervisor, inspector, employee or visitor
# SESSION_VISITOR_PERSON_ID: must be left empty
# SESSION_VISITOR_ROLE:      visitor
SESSION_SOURCE=default
SESSION_DEFAULT_PERSON_ID=1
SESSION_DEFAULT_ROLE=administrator
SESSION_VISITOR_PERSON_ID=
SESSION_VISITOR_ROLE=visitor

# PostGraphile
# POSTGRAPHILE_SCHEMAS: comma-separated list of database schemas that contain GraphQL API objects
POSTGRAPHILE_SCHEMAS=api

# Transaction settings
# PGSETTINGS_STATEMENT_TIMEOUT: timeout in milliseconds (0 means disabled timeout)
PGSETTINGS_STATEMENT_TIMEOUT=0

# Cron jobs schedule
# CRON_JOBS:                'on' or 'off'
# Cron patterns and ranges: https://www.npmjs.com/package/cron#usage-basic-cron-usage
CRON_JOBS=off
CRON_PATTERN_DUMP=0 0 1 * * *
CRON_PATTERN_DIFF=0 0 2 * * *
CRON_PATTERN_REFRESH=0 0 3 * * *
CRON_PATTERN_DASHBOARD=0 0 4 * * *

# Redmine
REDMINE_URL=https://redminesf.senado.gov.br/redmine/projects/solicitacoes-de-servico/issues.json?set_filter=1&page=
REDMINE_API_KEY=
